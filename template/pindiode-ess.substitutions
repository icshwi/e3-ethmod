           { $(P) ,I2C1:IOExp1: ,2   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:IOExp1: ,3   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
