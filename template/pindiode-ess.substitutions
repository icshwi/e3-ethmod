           { $(P) ,I2C1:IOExp1: ,7   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:IOExp1: ,8   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:IOExp1: ,9   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:IOExp1: ,10  ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
