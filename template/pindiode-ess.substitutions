file AKI2C_LTC2991_dev.template
{
   pattern { P    ,R           ,PORT    ,ADDR     ,TIMEOUT     }
           { $(P) ,I2C1:VMon1: ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
}

file AKI2C_LTC2991_port.template
{
   pattern { P    ,R           ,N    ,PORT    ,ADDR     ,TIMEOUT     }
           { $(P) ,I2C1:VMon1: ,V1   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V2   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V3   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V4   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V5   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V6   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V7   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,V8   ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,VCC  ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
           { $(P) ,I2C1:VMon1: ,TINT ,$(PORT) ,$(ADDR)  ,$(TIMEOUT)  }
}

